        "FROM ACCOUNT a\n",
        "LEFT JOIN TRANS t ON t.account_id = a.account_id\n",
        "LEFT JOIN LOAN l ON l.account_id = a.account_id\n",
        "WHERE l.loan_id IS NOT NULL"
